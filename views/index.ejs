        <section class="content-section container" >
            <!-- <img src="images/israelStore2.png" height="100%" width="100%" alt=""> -->
            <script>
                var i = 0
                var images = []
                var time = 3000
    
                images[0] = 'images/israelStore2.png'
                images[1] = 'images/JBLSpeaker.png'
                images[2] = 'images/oraimoSpeaker.png'
                images[3] = 'images/oraimoSpeaker2.png'
                images[4]= 'images/tandGSpaker.png'
    
                function changeImg(){
                    document.slide.src = images[i]
    
                    if (i<images.length - 1){
                        i++
                    }
                    else{
                        i=0;
                    }
                    setTimeout("changeImg()", time)
    
                }
                window.onload=changeImg
            </script>
            <img name="slide" width="100%" height="100%" alt="">

            <h1 >Home Of Quality Sounds</h1>

        </section>

        







